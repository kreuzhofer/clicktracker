# Development Environment Variables

# Application
NODE_ENV=development
PORT=3001

# Database Configuration
POSTGRES_DB=campaign_tracker_dev
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/campaign_tracker_dev

# Redis Configuration
REDIS_URL=redis://localhost:6380

# JWT Configuration
JWT_SECRET=your-development-jwt-secret-key-change-this
JWT_EXPIRES_IN=7d

# YouTube API Configuration
YOUTUBE_API_KEY=your-youtube-api-key

# Frontend Configuration
REACT_APP_API_URL=http://localhost:18001

# Logging
LOG_LEVEL=debug

# CORS Configuration
CORS_ORIGIN=http://localhost:3002

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100